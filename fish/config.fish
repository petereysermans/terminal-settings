# GIT aliases
alias gac="git add -A; and git commit -v"
alias gs="git status"

alias gp="git push --follow-tags"
alias gpo="git push origin --follow-tags"
alias gpom="git push origin master --follow-tags"

alias gl="git pull --follow-tags"
alias glo="git pull origin --follow-tags"
alias glom="git pull origin master --follow-tags"

alias gb="git branch"
alias gc="git checkout"

alias work="cd ~/Projects"
